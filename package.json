{
  "name": "jadin",
  "version": "0.0.0",
  "description": "A library for parsing MIDI files",
  "main": "dist/jadin.js",
  "scripts": {
    "prebuild": "rm -rf dist/ && mkdir dist/",
    "build": "babel src/ -d dist/ && browserify dist/Midi.js -s Midi > dist/jadin.js",
    "test": "mocha test/ --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blakeley/jadin.git"
  },
  "keywords": [
    "MIDI"
  ],
  "author": "Brandon Blakeley",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/blakeley/jadin/issues"
  },
  "homepage": "https://github.com/blakeley/jadin",
  "devDependencies": {
    "babel": "^5.8.29",
    "browserify": "^12.0.1",
    "chai": "^1.10.0",
    "chai-change": "^1.0.0",
    "mocha": "^2.1.0"
  }
}

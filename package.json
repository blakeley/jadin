{
  "name": "jadin",
  "version": "0.0.0",
  "description": "A library for parsing MIDI files",
  "main": "dist/jadin.umd.js",
  "module": "dist/jadin.es5.js",
  "typings": "dist/types/jadin.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist/ && mkdir dist/",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blakeley/jadin.git"
  },
  "keywords": [
    "MIDI"
  ],
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/blakeley/jadin/issues"
  },
  "homepage": "https://github.com/blakeley/jadin",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.5",
    "babel": "^5.8.29",
    "browserify": "^12.0.1",
    "chai": "^4.2.0",
    "chai-change": "^1.0.0",
    "lodash.camelcase": "^4.3.0",
    "mocha": "^7.1.1",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-node": "^8.8.1",
    "typedoc": "^0.12.0"
  },
  "dependencies": {
    "typescript": "^3.8.3"
  }
}
